{
  "name": "@geut/randombytes",
  "version": "1.0.0",
  "description": "randombytes from node that works in the browser",
  "type": "module",
  "main": "./dist/node.cjs",
  "browser": "./dist/browser.cjs",
  "exports": {
    ".": {
      "require": "./dist/node.cjs",
      "import": "./src/node.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "test": "uvu",
    "posttest": "npm run lint",
    "lint": "standard",
    "prepublishOnly": "npm test && npm run build"
  },
  "devDependencies": {
    "standard": "^17.0.0",
    "tsup": "^6.3.0",
    "uvu": "^0.5.3"
  },
  "tsup": {
    "entry": [
      "src/node.js",
      "src/browser.js"
    ],
    "format": [
      "cjs",
      "iife"
    ],
    "globalName": "randomBytes",
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "standard": {
    "env": [
      "node",
      "browser"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geut/randombytes.git"
  },
  "keywords": [
    "randombytes",
    "geut",
    "module"
  ],
  "author": {
    "name": "GEUT",
    "email": "contact@geutstudio.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geut/randombytes/issues"
  },
  "homepage": "https://github.com/geut/randombytes#readme",
  "publishConfig": {
    "access": "public"
  }
}
